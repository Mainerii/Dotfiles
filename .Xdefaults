URxvt.imLocale: 	fi_FI.utf8
URxvt.iso14755: 	false
URxvt.iso14755_52: 	false

URxvt.font:     	xft:FreeMono:size=11:antialias=true
URxvt.boldFont: 	xft:FreeMono:bold:size=11:antialias=true

urxvt*termName:		rxvt
urxvt*scrollBar:	false
urxvt*depth:		32
urxvt*background:	rgba:1600/1b00/2100/ee00

! URxvt plugins
URxvt.perl-ext-common:			tabbedex

! Fix URxvt word by word movement
URxvt.keysym.Control-Left:		\033[1;5D
URxvt.keysym.Control-Right:		\033[1;5C

! Tabbedex configs

URxvt.tabbed.no-tabbedex-keys:		true
URxvt.tabbed.new-button: 		true
URxvt.tabbed.autohide: 			true
URxvt.keysym.Control-t: 		perl:tabbedex:new_tab
URxvt.keysym.Control-Shift-Right:	perl:tabbedex:next_tab
URxvt.keysym.Control-Shift-Left:	perl:tabbedex:prev_tab

URxvt.tabbed.tabbar-fg:   		#cccccc
URxvt.tabbed.tabbar-bg:   		#1a1a1a
URxvt.tabbed.tab-fg:      		#466a8e
URxvt.tabbed.tab-bg:      		#1a1a1a
URxvt.tabbed.bell-tab-fg: 		#ffffff
URxvt.tabbed.bell-tab-bg: 		#3399ff
URxvt.tabbed.bell-fg:     		#ffffff
URxvt.tabbed.bell-bg:     		#1a1a1a
URxvt.tabbed.title-fg:    		#1a1a1a
URxvt.tabbed.title-bg:    		#1a1a1a

! special
*.foreground:   #e5e6e5
*.background:   #20201d
*.cursorColor:  #a6a28c

! black
*.color0:	#20201d
*.color8:	#7d7a68

! red
*.color1:	#d73737
*.color9:	#d73737

! green
*.color2:	#60ac39
*.color10:	#60ac39

! yellow
*.color3:	#cfb017
*.color11:	#cfb017

! blue
*.color4:	#6684e1
*.color12:	#6684e1

! magenta
*.color5:	#b854d4
*.color13:	#b854d4

! cyan
*.color6:	#1fad83
*.color14:	#1fad83

! white
*.color7:	#a6a28c
*.color15:	#fefbec

